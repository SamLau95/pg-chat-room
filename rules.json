{
  "rules": {
    "admins": {
      ".read": "root.child('admins/' + auth.uid).exists()"
    },

    "constants": {
      // ".read": true,
      ".read": "root.child('admins/' + auth.uid).exists()",
      ".write": "root.child('admins/' + auth.uid).exists()"
    },

    "studies": {
      ".read": "root.child('admins/' + auth.uid).exists()",
      ".write": "root.child('admins/' + auth.uid).exists()"
    },

    "$study": {
      ".read": "root.child('admins/' + auth.uid).exists()",
      ".write": "root.child('admins/' + auth.uid).exists()",

      "$roomType": {
        "rooms": {
          "$roomId": {
            "createdAt": {
              ".read": true,
              ".write": true
            },
            "messages": {
              "$messageId": {
                ".read": true,
                ".write": true
              }
            },
            "users": {
              "$userId": {
                ".read": true,
                ".write": true
              }
            }
          },
          ".read": true,
          ".write": true
        },
        "users": {
          ".read": true,
          ".write": true
        }
      }
    }
  }
}
